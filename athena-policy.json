{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "athena:ListWorkGroups"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "athena:StartQueryExecution",
                "athena:GetQueryExecution",
                "athena:GetQueryResults",
                "athena:StopQueryExecution",
                "athena:GetWorkGroup",
                "athena:GetQueryResultsStream"
            ],
            "Resource": [
                "arn:aws:athena:us-east-2:472360887532:workgroup/*",
                "arn:aws:athena:*:*:datacatalog/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::ps-labs-dev-agent-v1/*",
                "arn:aws:s3:::ps-labs-dev-agent-v1",
                "arn:aws:s3:::ps-labs-prod-agent-v1/*",
                "arn:aws:s3:::ps-labs-prod-agent-v1",
                "arn:aws:s3:::ps-labs-*/*",
                "arn:aws:s3:::ps-labs-*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "glue:GetDatabase",
                "glue:GetTable",
                "glue:GetTables",
                "glue:GetPartitions"
            ],
            "Resource": "*"
        }
    ]
}
