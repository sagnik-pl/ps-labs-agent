# LLM Provider
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

# Athena S3 Output - Development
ATHENA_S3_OUTPUT_LOCATION_DEV=s3://your-athena-output-bucket-dev/
GLUE_DATABASE_DEV=your_glue_database_name_dev

# Athena S3 Output - Production
ATHENA_S3_OUTPUT_LOCATION_PROD=s3://your-athena-output-bucket-prod/
GLUE_DATABASE_PROD=your_glue_database_name_prod

# Firebase - AWS Secrets Manager
FIREBASE_SECRET_NAME_DEV=ps-labs-firebase-dev
FIREBASE_SECRET_NAME_PROD=ps-labs-firebase-prod

# Application
ENVIRONMENT=development
LOG_LEVEL=INFO
